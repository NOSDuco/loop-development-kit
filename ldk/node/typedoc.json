{
  "out": "docs",
  "includeDeclarations": true,
  "excludeExternals": true,
  "excludePrivate": true,
  "mode": "file",
  "toc": [],
  "exclude": [
    "src/grpc/**/*",
    "src/shared/**/*",
    "coverage/**/*",
    "jest.config.js",
    "**/*.test.ts"
  ],
  "listInvalidSymbolLinks": true,
  "inputFiles": ["src/index.ts", "docsExtra/*.md"],
  "stripInternal": true,
  "theme": "pages-plugin",
  "name": "Loop Development Kit for Node.js",
  "pages": {
    "groups": [
      {
        "title": "Concepts",
        "pages": [
          {
            "title": "Loops",
            "source": "./docsExtra/loop.md"
          },
          {
            "title": "Logging",
            "source": "./docsExtra/logging.md"
          },
          {
            "title": "Configuration",
            "source": "./docsExtra/configuration.md"
          },
          {
            "title": "Terminology",
            "source": "./docsExtra/terminology.md"
          }
        ]
      }
    ],
    "theme": "pages-plugin"
  }
}
